{
    "clang.cxxflags": [
        "-std=c++11",
        "-Wall",
        "-Wno-pragma-once-outside-header",
        "-Iinclude",
        "-Ilib/gfm/include"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "build": true,
        "build-sh": true
    },
    "cmake.preferredGenerators": [
        "Unix Makefiles",
        "NMake Makefiles"
    ],
    "C_Cpp.clang_format_style": " {BasedOnStyle: Google, IndentWidth: 4 } ",
    "files.associations": {
        "cmath": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory0": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp",
        "list": "cpp",
        "memory": "cpp",
        "string": "cpp",
        "tuple": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "xhash": "cpp",
        "xmemory": "cpp",
        "strstream": "cpp",
        "cctype": "cpp",
        "functional": "cpp",
        "sstream": "cpp",
        "xfunctional": "cpp",
        "algorithm": "cpp",
        "ctime": "cpp",
        "fstream": "cpp",
        "iomanip": "cpp",
        "map": "cpp",
        "set": "cpp",
        "xlocmon": "cpp",
        "xloctime": "cpp",
        "xtree": "cpp"
    }
}